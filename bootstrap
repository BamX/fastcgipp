export VERSION="2.0alpha"
export DATE="Feb 20, 2009"
mv configure.ac configure.ac.bak
git --no-pager log -M --stat > ChangeLog
sed "s/ACTUALVERSION/$VERSION/" configure.ac.bak > configure.ac
aclocal -I config
libtoolize --force
autoheader
autoconf
automake -a
doxygen doc/doxygen
mv configure.ac.bak configure.ac
